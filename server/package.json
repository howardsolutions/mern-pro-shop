{
  "name": "iproshop-server",
  "version": "1.0.0",
  "description": "eCommerce application server with node expressjs",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "kill -9 $(lsof -ti:3001,4000,4500,5000,5001) && node server.js",
